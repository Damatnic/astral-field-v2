{
  "name": "astral-field-v2.1",
  "version": "2.1.0",
  "private": true,
  "scripts": {
    "precommit": "npm run lint && npm run type-check",
    "prepush": "npm run test",
    "dev": "next dev",
    "build": "prisma generate && next build",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "preview": "next build && next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next dist node_modules/.cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "db:generate": "prisma generate",
    "db:migrate": "tsx scripts/db-migrate.ts",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:migrate:status": "prisma migrate status",
    "db:seed": "prisma db seed",
    "db:seed:prod": "tsx scripts/db-seed-production.ts",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:optimize": "tsx scripts/optimize-database.ts",
    "db:backup": "tsx scripts/database-backup.ts",
    "db:backup:list": "tsx scripts/database-backup.ts --list",
    "db:health": "tsx scripts/database-health-check.ts",
    "db:health:detailed": "tsx scripts/database-health-check.ts --detailed",
    "league:seed": "tsx prisma/seed.ts --league 2025 --week 2",
    "verify:setup": "tsx scripts/verify-setup.ts",
    "deploy": "vercel --prod",
    "deploy:staging": "vercel",
    "deploy:preview": "vercel --no-wait",
    "env:pull": "vercel env pull .env.local",
    "env:list": "vercel env ls",
    "logs": "vercel logs",
    "logs:function": "vercel logs --follow",
    "perf:test": "node scripts/performance/run-performance-tests.js",
    "perf:test:quick": "node scripts/performance/run-performance-tests.js --suite quick",
    "perf:test:load": "node scripts/performance/run-performance-tests.js --suite load",
    "perf:test:draft": "node scripts/performance/load-test-draft.js",
    "perf:test:scoring": "node scripts/performance/load-test-scoring.js",
    "perf:test:api": "node scripts/performance/load-test-api.js",
    "perf:test:memory": "node scripts/performance/memory-test.js",
    "perf:test:database": "node scripts/performance/database-performance.js",
    "perf:analyze": "npm run build && npm run perf:test",
    "perf:production": "node scripts/performance/run-performance-tests.js --url https://astralfield.com",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "docker:build": "docker build -t astralfield .",
    "docker:run": "docker run -p 3000:3000 astralfield",
    "docker:dev": "docker-compose up -d",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "postinstall": "prisma generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@hello-pangea/dnd": "^18.0.1",
    "@prisma/client": "^5.19.1",
    "@sentry/nextjs": "^10.11.0",
    "@sentry/tracing": "^7.120.4",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/ioredis": "^4.28.10",
    "@types/sanitize-html": "^2.16.0",
    "@upstash/redis": "^1.35.3",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.12",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.13.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cron": "^3.1.7",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "framer-motion": "^11.11.1",
    "ioredis": "^5.7.0",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.544.0",
    "nanoid": "^5.0.7",
    "next": "^14.2.10",
    "next-auth": "^4.24.8",
    "openai": "^4.67.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-hot-toast": "^2.4.1",
    "react-query": "^3.39.3",
    "resend": "^4.0.0",
    "sanitize-html": "^2.17.0",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "stripe": "^17.1.0",
    "tailwind-merge": "^3.3.1",
    "tsx": "^4.19.0",
    "web-vitals": "^5.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^14.2.10",
    "@playwright/test": "^1.47.2",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cron": "^2.4.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.16.5",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.10",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "msw": "^2.11.2",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "prisma": "^5.19.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.5.4"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run test:ci"
    }
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "fantasy-football",
    "nextjs",
    "typescript",
    "prisma",
    "postgresql",
    "real-time",
    "ai-powered",
    "sports"
  ],
  "author": {
    "name": "AstralField Team",
    "email": "team@astralfield.com",
    "url": "https://astralfield.com"
  },
  "license": "MIT",
